{
  "name": "borealis-pg-heroku-cli-plugin",
  "description": "Advanced interactions with Borealis Isolated Postgres add-ons",
  "version": "0.1.0",
  "author": "Borealis Data Solutions",
  "bugs": "https://github.com/OldSneerJaw/borealis-pg-heroku-cli-plugin/issues",
  "dependencies": {
    "@heroku-cli/command": "^8.5.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "cli-ux": "^5.6.3",
    "http-call": "^5.3.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@heroku-cli/schema": "^1.0.25",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.20.18",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^11.0.4",
    "mocha": "^9.0.3",
    "nock": "^13.1.1",
    "nyc": "^15.1.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/OldSneerJaw/borealis-pg-heroku-cli-plugin",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "OldSneerJaw/borealis-pg-heroku-cli-plugin",
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
